    <declare-styleable name="Pulsator4Droid">
        <attr name="pulse_count" format="integer" />
        <attr name="pulse_duration" format="integer" />
        <attr name="pulse_color" format="color" />
        <attr name="pulse_repeat" format="integer" />
        <attr name="pulse_maxScale" format="float" />
        <attr name="pulse_startFromScratch" format="boolean" />
        <attr name="pulse_interpolator" format="enum">
            <enum name="Linear" value="0" />
            <enum name="Accelerate" value="1" />
            <enum name="Decelerate" value="2" />
            <enum name="AccelerateDecelerate" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="ChatView">
        <attr name="showSenderLayout" format="boolean" />
        <attr name="leftBubbleLayoutColor" format="color" />
        <attr name="rightBubbleLayoutColor" format="color" />
        <attr name="leftBubbleTextColor" format="color" />
        <attr name="rightBubbleTextColor" format="color" />
        <attr name="chatViewBackgroundColor" format="color" />
        <attr name="timeTextColor" format="color" />
        <attr name="chatViewBackground" format="integer" />
        <attr name="textSize" format="dimension" />
        <attr name="showLeftBubbleIcon" format="boolean" />
        <attr name="showRightBubbleIcon" format="boolean" />
        <attr name="showSenderName" format="boolean" />
        <attr name="senderNameTextColor" format="color" />
    </declare-styleable>
    <declare-styleable name="ImFlexboxLayout">
        <attr name="viewPartMain" format="reference"></attr>
        <attr name="viewPartSlave" format="reference"></attr>
    </declare-styleable>

</resources>